start =
    expression

validchar
    = [0-9a-zA-Z_?!+\-=@#$%^&*/.]

spaces = " "*

left_brace = "("
right_brace = ")"

atom =
    spaces chars:validchar+ spaces
        { return chars.join(""); }

exprlist = list:expression*
{return list;}

expression = a:atom
{return a;}
/spaces left_brace list:exprlist spaces right_brace
{return list;}
